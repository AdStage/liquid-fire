{{#liquid-bind (-get-dynamic-var "outletState")
      containerId=containerId
      versionEquality=versionEquality
      matchContext=(hash outletName=outletName helperName="liquid-outlet")
      class=class
      use=use
      rules=rules
      containerless=containerless
      growDuration=growDuration
      growPixelsPerSecond=growPixelsPerSecond
      growEasing=growEasing
      enableGrowth=enableGrowth
      as |version| ~}}
      {{#-with-dynamic-var "outletState" version ~}}
        {{outlet outletName}}
      {{~/-with-dynamic-var}}
{{~/liquid-bind~}}
