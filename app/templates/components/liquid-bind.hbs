{{#if containerless}}
  {{~#liquid-versions value=(readonly attrs.value) use=use
                     name="liquid-bind" renderWhenFalse=true class=class as |version| ~}}
    {{~version ~}}
  {{/liquid-versions ~}}
{{else}}
  {{~#liquid-container
      class=class
      growDuration=growDuration
      growPixelsPerSecond=growPixelsPerSecond
      growEasing=growEasing
      enableGrowth=enableGrowth
      as |container| ~}}
    {{~#liquid-versions value=(readonly attrs.value) notify=container use=use
                       name="liquid-bind" renderWhenFalse=true as |version| ~}}
      {{~version ~}}
    {{/liquid-versions ~}}
  {{/liquid-container ~}}
{{/if}}
