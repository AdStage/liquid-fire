{{#if containerless}}
  {{~#liquid-versions value=value use=use
                     name="liquid-bind" renderWhenFalse=true innerClass=innerClass as |version| ~}}
    {{~version ~}}
  {{/liquid-versions ~}}
{{else}}
  {{~#liquid-container id=innerId class=innerClass as |container| ~}}
    {{~#liquid-versions value=value notify=container use=use
                       name="liquid-bind" renderWhenFalse=true as |version| ~}}
      {{~version ~}}
    {{/liquid-versions ~}}
  {{/liquid-container ~}}
{{/if}}
