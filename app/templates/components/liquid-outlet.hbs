{{~#get-outlet-state outletName watchModels=attrs.watchModels as |outletState|~}}
  {{#liquid-bind outletState
      id=id
      class=class
      use=use
      name="liquid-outlet"
      outletName=outletName
      containerless=containerless
      growDuration=growDuration
      growPixelsPerSecond=growPixelsPerSecond
      growEasing=growEasing
      enableGrowth=enableGrowth
      willAnimate=attrs.willAnimate
      didAnimate=attrs.didAnimate
    as |version| ~}}

      {{#set-outlet-state outletName version.outletState~}}
        {{outlet outletName}}
      {{~/set-outlet-state}}

  {{~/liquid-bind~}}
{{/get-outlet-state}}
