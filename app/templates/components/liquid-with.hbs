{{#if containerless}}
  {{~#liquid-versions value=value use=use name=name as |version| ~}}
    {{~yield version ~}}
  {{/liquid-versions~}}
{{else}}
  {{#liquid-container id=innerId class=innerClass as |container| ~}}
    {{~#liquid-versions value=value notify=container use=use name=name as |version| ~}}
      {{~yield version ~}}
    {{/liquid-versions~}}
  {{/liquid-container}}
{{/if}}
