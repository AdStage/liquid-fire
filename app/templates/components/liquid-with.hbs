{{#if containerless}}
  {{~#liquid-versions value=value use=use name=name innerClass=innerClass as |version| ~}}
    {{~yield version ~}}
  {{/liquid-versions~}}
{{else}}
  {{#liquid-container
      id=innerId
      class=innerClass
      growDuration=growDuration
      growPixelsPerSecond=growPixelsPerSecond
      growEasing=growEasing
      enableGrowth=enableGrowth
      as |container| ~}}
    {{~#liquid-versions value=(readonly attrs.value) notify=container use=use name=name as |version| ~}}
      {{~yield version ~}}
    {{/liquid-versions~}}
  {{/liquid-container}}
{{/if}}
