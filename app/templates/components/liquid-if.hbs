{{#if containerless}}
  {{#liquid-versions value=value name=helperName
                       use=use renderWhenFalse=hasInverse as |valueVersion|}}
    {{#if valueVersion}}
      {{yield}}
    {{else}}
      {{lf-yield-inverse}}
    {{/if}}
  {{/liquid-versions}}
{{else}}
  {{#liquid-container id=innerId class=innerClass as |container|}}
    {{#liquid-versions value=value notify=container name=helperName
                       use=use renderWhenFalse=hasInverse as |valueVersion|}}
      {{#if valueVersion}}
        {{yield}}
      {{else}}
        {{lf-yield-inverse}}
      {{/if}}
    {{/liquid-versions}}
  {{/liquid-container}}
{{/if}}
