{{#liquid-container class=innerClass as |container|}}
  {{#liquid-versions value=value notify=container name="liquid-if"
                     use=use renderWhenFalse=hasInverse as |valueVersion|}}
    {{#if valueVersion}}
      {{yield}}
    {{else}}
      {{lf-yield-inverse}}
    {{/if}}
  {{/liquid-versions}}
{{/liquid-container}}

